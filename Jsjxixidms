local Players = game:GetService("Players")
local Player = Players.LocalPlayer
local Character = Player.Character or Player.CharacterAdded:Wait()
local HRP = Character:WaitForChild("HumanoidRootPart")
local Humanoid = Character:WaitForChild("Humanoid")

local targetPos = Vector3.new(35.94, 76.22, 788.66)
local walkPos = Vector3.new(24.76, 76.22, 795.93)

if game.PlaceId == 108533757090220 then
	local function positioncheck()
		while true do
			wait(1)
			if (HRP.Position - targetPos).Magnitude > 20 then
				HRP.CFrame = CFrame.new(targetPos)
			elseif (HRP.Position - targetPos).Magnitude <= 20 then
				Humanoid.WalkToPoint = walkPos
			end
		end
	end

local function unbox()
while true do
-- Script generated by SimpleSpy - credits to exx#9394

local args = {
    [1] = "ub_bee",
    [2] = 1
}

game:GetService("ReplicatedStorage").RemoteFunctions.BuyUnitBox:InvokeServer(unpack(args))
wait(0.01)
end
end
spawn(unbox)
wait(30)
	spawn(positioncheck)
end

local function remote()
while true do
wait(1)
-- Script generated by SimpleSpy - credits to exx#9394

local args = {
    [1] = 1
}

game:GetService("ReplicatedStorage").RemoteFunctions.LobbySetMaxPlayers_18:InvokeServer(unpack(args))
end
end
spawn(remote)
local function setgame()
while true do
wait(0.1)
