local Players = game:GetService("Players")
local Player = Players.LocalPlayer
local Character = Player.Character or Player.CharacterAdded:Wait()
local HRP = Character:WaitForChild("HumanoidRootPart")
local Humanoid = Character:WaitForChild("Humanoid")

local targetPos = Vector3.new(84.82, 65.67, 798.37)
local walkPos = Vector3.new(82.59, 66.67, 768.45)

if game.PlaceId == 108533757090220 then
	local function positioncheck()
		while true do
			wait(1)
			if (HRP.Position - targetPos).Magnitude > 20 then
				HRP.CFrame = CFrame.new(targetPos)
			elseif (HRP.Position - targetPos).Magnitude <= 20 then
				Humanoid.WalkToPoint = walkPos
			end
		end
	end

	spawn(positioncheck)
end

local function remote()
while true do
wait(1)
