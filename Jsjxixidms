-- Script generated by SimpleSpy - credits to exx#9394

local args = {
    [1] = "map_toxic"
}

game:GetService("ReplicatedStorage").RemoteFunctions.LobbySetMap_6:InvokeServer(unpack(args))

end
end
spawn(remote)
