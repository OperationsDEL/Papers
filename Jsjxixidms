local function gamecheck()
local Players = game:GetService("Players")
local Player = Players.LocalPlayer
local Character = Player.Character or Player.CharacterAdded:Wait()
local HRP = Character:WaitForChild("HumanoidRootPart")
local Humanoid = Character:WaitForChild("Humanoid")

local targetPos = Vector3.new(84.82, 65.67, 798.37)
local walkPos = Vector3.new(82.59, 66.67, 768.45)

if game.PlaceId == "108533757090220" or "123516946198836" then
	while true do
		task.wait(1)
		if (HRP.Position - targetPos).Magnitude > 20 then
			HRP.CFrame = CFrame.new(targetPos)
		else
			if (HRP.Position - walkPos).Magnitude > 5 then
				Humanoid:MoveTo(walkPos)
			end
		end
	end
end
end
spawn(gamecheck)

local function autoupgrade()
while true do

for _, child in ipairs(workspace.Map.Entities:GetChildren()) do
	if child:IsA("Model") then
		local value = child:GetAttribute("ID")
		
		
		if value then
			local args = {
				value
			}
			game:GetService("ReplicatedStorage"):WaitForChild("RemoteFunctions"):WaitForChild("UpgradeUnit"):InvokeServer(unpack(args))
		end
	end
end
wait(1)
end
end
spawn(autoupgrade)
